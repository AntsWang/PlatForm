:global(.noData) {
    text-align: center;
    margin-top: 150px;
}

a {
    text-decoration: none;
}
.item{
    height:40px;
    display: flex;
    border-bottom: 1px solid #ccc;
    background:#fff;
    padding:0 10px 0 10px;
}
.hello{
    margin-top: 40px;
}

.record_nav {
    top: 40px;
    left: 0;
    height: 40px;
    background: #888;
    display: flex;
}

.nav_link {
    display: flex;
    flex: 1;
    justify-content: center;
    align-items: center;
}

:global(.active) {
    color: #f00;
    border-bottom: 2px solid blue;
}

.headname {
    height: 40px;
    display: flex;
    border-bottom: 1px solid #ccc;
    background:#efeff4;
    border-top: 1px solid #ccc;
    padding:0 10px 0 10px;
}
.name{
    flex:5;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}


.win {
    flex: 3;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}
.titlewin{
    flex: 3;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    margin-right: 25px;
}
.container {
    min-height: 100%;
}

.listTitle {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
}
.right{
    display: flex;
    align-items: center;
}
.right img{
    width:20px;
    height:20px;
    margin-left: 5px;
}

.title{
    height: 44px;
    line-height: 44px;
    text-align: center;
    font-size: 14px;
    background: #fff;
    border-bottom: 1px solid #ddd;
}
.head{
    height:40px;
    display: flex;
    align-items: center;
    padding: 0 10px 0 10px;
    background:#efeff4;
}
.head input{
    height:20px;
    font-size: 12px;
    text-align: center;
}
.search{
    height:20px;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #30bf27;
    border-radius: 4px;
    color:#fff;
    padding: 0 10px;
}
.timeContainer{
    flex:5;
    display: flex;
    align-items: center; 
 }
 .time{
     flex: 1;
     display: flex;
     justify-content: center;
     align-items: center;
     border: 1px solid #ddd;
     border-radius: 5px;
     margin-right: 20px;
 }
 .allTotal{
    display: flex;
    height: 40px;
    align-items: center;
    background: #fff;
    border-bottom: 1px solid #ddd;
    padding: 0 10px 0 10px;
    background: #ddd;
}
.totalWin{
    flex:1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    color:#30bf27;
}
.totalName{
    flex:3;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.allWin{
    flex:1;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    color:#fd4d4d;
    margin-right: 20px;
}
.allLose{
    flex:1;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    margin-right: 20px;
    color:#338ffe;
}
.fu{
    color:#fd4d4d;
    margin-right: 25px;
}
.zheng{
    color:#338ffe;
    margin-left: 25px;
}