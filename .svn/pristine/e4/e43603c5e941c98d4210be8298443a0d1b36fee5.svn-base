.head{
    height:40px;
    display: flex;
    border-bottom: 1px solid #ccc;
    align-items: center;
    padding: 0 10px 0 10px;
    background:#fff;
}
.head input{
    height:20px;
    font-size: 12px;
    text-align: center;
}
.search{
    width:36px;
    height:20px;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #30bf27;
    border-radius: 4px;
    color:#fff;
}
.timeContainer{
   flex:5;
   display: flex;
   align-items: center; 
}
.time{
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 1px solid #ddd;
    border-radius: 5px;
    margin-right: 20px;
}
.type{
    flex:3;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.total{
    flex:1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}
.back{
    flex:1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    color: red;
}
.item{
    display: flex;
    height:40px;
    background:#fff;
    border-bottom: 1px solid #ddd;
    align-items: center;
    padding:0 10px 0 10px;
}
.name{
    flex:1;
}
.score{
    flex:1;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}
.score  img{
    width:20px;
}
.noChildren{
    text-align: center;
    margin-top: 100px;
    font-size: 14px;
}