.modal{
    position: absolute;
    top: 0;
    left: 0;
    width:100%;
    height:100%;
    background: #000;
    opacity: 0.5;
    display:flex;
    justify-content:center;
    align-items: center;

}
.alertContainer{
    width: 60%;
    color:#808080;
    background: #fff;
    border-radius: 10px;
}
.alertTitle{
    width:100%;
    height:30px;
    border-bottom: 1px solid #000;
    color: #808080;
    text-align: center;
    vertical-align: middle;
}
.alertContent{
    width: 100%;
    height:60px;
    text-align: center;
    vertical-align: middle;
}
.alertBtn{
    width:100%;
    height:30px;
    display: flex;
    border-top: 1px solid #000;
}
.alertBtnitem{
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
}
:global(.weui-dialog__bd){
    font-size: 14px!important;
}